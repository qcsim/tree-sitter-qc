  "grammars": [
    {
      "name": "qc",
      "camelcase": "QC",
      "title": "QC",
      "scope": "source.qc",
      "file-types": ["qc"],
      "injection-regex": "^qc$",
      "class-name": "TreeSitterQc"
    }
  ],
  "metadata": {
    "version": "0.2.0",
    "license": "0BSD",
    "description": "The instructions language for QCSim",
    "authors": [
      {
        "name": "noClaps",
        "email": "contact@zerolimits.dev",
        "url": "https://zerolimits.dev/"
      }
    ],
    "links": {
      "repository": "https://github.com/qcsim/tree-sitter-qc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
